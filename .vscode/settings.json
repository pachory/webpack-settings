{
    "editor.formatOnSave": false,
    "eslint.nodePath": "./node_modules/eslint",
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        {"language": "javascript", "autoFix": true},
        {"language": "typescript", "autoFix": true}
    ],
    "puglint.enable": true,
    "puglint.run": "onSave",
    "puglint.config": {
        "extends": "clock",
        "validateAttributeQuoteMarks": "\""
    }
}